
#include <stdio.h>
#include<math.h>
int main() {
    float a,b,c,r1,r2;
    scanf("%f%f%f",&a,&b,&c);
    float d=b*b-4*a*c;
    if(d<0)
    {
        printf("Roots are not possible");
    }
    else
    {
    float rl=(-b+sqrt(b*b-4*a*c))/(2*a);
    float r2=(-b-sqrt(b*b-4*a*c))/(2*a);
    printf("root1=%f\n",r1);
    printf("root2=%f",r2);
    }
    return 0; 
}
